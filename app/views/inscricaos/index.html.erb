<p id="notice"><%= notice %></p>

<h1>Inscricaos</h1>

<table>
  <thead>
    <tr>
      <th>Nome</th>
      <th>Cpf</th>
      <th>Email</th>
      <th>Datanascimento</th>
      <th>Evento</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @inscricaos.each do |inscricao| %>
      <tr>
        <td><%= inscricao.nome %></td>
        <td><%= inscricao.cpf %></td>
        <td><%= inscricao.email %></td>
        <td><%= inscricao.datanascimento %></td>
        <td><%= inscricao.evento_id %></td>
        <td><%= link_to 'Show', inscricao %></td>
        <td><%= link_to 'Edit', edit_inscricao_path(inscricao) %></td>
        <td><%= link_to 'Destroy', inscricao, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Inscricao', new_inscricao_path %>
